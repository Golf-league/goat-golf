<td><%= link_to score.user.nickname, user_path(score.user.id) %></td>
<td>
  <%= score.course %>
  <% if score.open_round %>
    <span class="float-sm-right text-danger">OPEN</span>
  <% end %>
</td>
<td><%= score.date.strftime("%d-%m-%Y") %></td>
<td class="hidden-sm-down"><%= score.handicap %></td>
<td><%= score.stableford %></td>
<td class="hidden-sm-down"><%= score.strokes %></td>

